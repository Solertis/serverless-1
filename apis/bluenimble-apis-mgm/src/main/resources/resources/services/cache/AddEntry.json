{
	    "verb": "post",
	"endpoint": "/cache/:provider/buckets/:bucket/entries",
	
	"spec": {
		"contentType": "application/json",
		"fields": {
			"provider": { "max": "100", "value": "default" },
			"bucket": 	{ "min": 4, "max": "20", "vType": "AlphaNumeric" },
			"payload": 	{ 
				"type": "Map",
				"fields": {
					"key": 		{  },
					"value": 	{ "type": "Raw" },
					"ttl": { "type": "Long", "min": 0, "required": "false", "value": "0" }
				}
			}
		}
	},
	
	"security": {
		"roles": ["admin", "developer"]
	},
	
	"spi": {
		"class": "com.bluenimble.platform.apis.mgm.spis.cache.AddEntrySpi"
	} 
	
}