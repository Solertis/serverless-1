{
	    "verb": "get",
	"endpoint": "/cache/:provider/buckets/:bucket",
	
	"spec": {
		"fields": {
			"provider": { "max": "100", "value": "default" },
			"bucket": 	{ "min": "4", "max": "20", "vType": "AlphaNumeric" },
			"start": 	{ "min": "0", "type": "Integer", "value": "0" },
			"page": 	{ "min": "1", "max": "50", "type": "Integer", "value": "20" }
		}
	},
	
	"security": {
		"roles": ["admin", "developer"]
	},
	
	"spi": {
		"class": "com.bluenimble.platform.apis.mgm.spis.cache.GetBucketSpi"
	} 
	
}