{
	    "verb": "get",
	"endpoint": "/static/::path",
	
	"security": {
		/* should turn true or remove this property as by default services are secure */
		"enabled": "false", 
		"roles": ["super"]
	},
	
	"media": {
		"*/*": {
			"processor": "stream"
		}
	},
	
	"spi": {
		"class": "core:ResourceSpi",
		"resources": {
			"root": "app"
		}
	} 
	
}