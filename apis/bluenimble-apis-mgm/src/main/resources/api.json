{
	"namespace": "mgm", 
	"name": "BlueNimble Management API",
	
	"security": {
		"schemes": {
			"signature": {}
		}
	},
	
	"features": {
		"media.default": {
			"templating": {
				"objects": {
					"TemplateTool": {
						"class": "com.bluenimble.platform.apis.mgm.media.TemplateTool"
					}
				}
			}
		}
	},
	
	"runtime":{
		"endpoint": "[ this.apis.sys.mgm | 'localhost:9090/%s/%s' ]"
	},
	
	"spi": {
		"class": "com.bluenimble.platform.apis.mgm.KeyStoreAwareApiSpi"
	}
	
}