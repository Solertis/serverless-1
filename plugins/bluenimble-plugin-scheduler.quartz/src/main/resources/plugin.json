{
	"name": "scheduler.quartz",
	"title": "Scheduler Implementation - Run Jobs - Quartz",
	
	"spi": {
		"class": "com.bluenimble.platform.server.plugins.scheduler.SchedulerPlugin",
		"properties": {
			"version": "[version]",
			"vendor": {
				"name": "BlueNimble, Inc",
				"links": [{
					"Read More": "https://www.bluenimble.com"
				}]
			},
			"scheduler": {
				"org.quartz.scheduler.instanceName": "DefaultScheduler",
				"org.quartz.scheduler.instanceId": "AUTO",
				"org.quartz.threadPool.threadCount": "[ vars.plugins.scheduler.threadPool | '10' ]"
			}
		}
	}
}	