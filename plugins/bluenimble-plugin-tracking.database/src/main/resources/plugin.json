{
	"namespace": "tracking.database",
	"name": "Requests Tracking implementation - Database backend",
	
	"spi": {
		"class": "com.bluenimble.platform.plugins.tracking.DatabaseBasedTrackingPlugin",
		"properties": {
			"version": "[version]",
			"vendor": {
				"name": "BlueNimble, Inc",
				"links": [{
					"Read More": "https://www.bluenimble.com"
				}]
			},
			"tracker": {
				"name": "database",
				"capacity": "[ this.tracker.capacity | '100' ]",
				"min": "[ this.tracker.minThreads | '5' ]",
				"max": "[ this.tracker.maxThreads | '10' ]",
				"keepAliveTime": "[ this.tracker.keepAliveTime | '30' ]"
			}
		}
	}
}	