{
	"namespace": "security.keystore",
	"name": "Space KeyStore Manager Implementation - Filesystem based keypairs storage",
	
	"spi": {
		"class": "com.bluenimble.platform.plugins.keystores.KeyStorePlugin",
		"properties": {
			"version": "[version]",
			"vendor": {
				"name": "BlueNimble, Inc",
				"links": [{
					"Read More": "https://www.bluenimble.com"
				}]
			},
			"closable": "false",
			
			"listener": {
				"delay": "[ this.listener.delay | '5' ]",
				"period": "[ this.listener.period | '300' ]",
				"file": "[ this.listener.file | 'space.keystore' ]",
				"flushRate": "[ this.listener.flushRate | '10' ]"
			}
		}
	}
}	