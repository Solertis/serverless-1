{
	"name": "security.keystore",
	"title": "Space KeyStore Manager Implementation - Filesystem based keypairs storage",
	
	"spi": {
		"class": "com.bluenimble.platform.plugins.keystores.KeyStorePlugin",
		"properties": {
			"version": "[version]",
			"vendor": {
				"name": "BlueNimble, Inc",
				"links": [{
					"Read More": "https://www.bluenimble.com"
				}]
			},
			"listener": {
				"pool": "10",
				"delay": "5",
				"period": "300",
				"file": "space.keystore",
				"flushRate": "10"
			},
			"closable": "false"
		}
	}
}	