{
	
	    "verb": "post",
	"endpoint": "/[[models]]",
		"name": "Create [[Model]]",
	
	"spec": {
		"fields": {
			"Content-Type": { "scope": "h", "lov": [ "application/json" ] },
			"payload": { "type": "Object", "strict": [[#eq ModelSpec.strict 'true']]"true"[[else]]"false"[[/eq]][[#if ModelSpec]][[#if ModelSpec.fields]], 
				"fields": [[json ModelSpec.fields]][[/if]][[/if]] 
			}
		}
	},
	
	"spi": {
		"class": "[[package]].[[models]].Create[[Model]]"
	}
	
}